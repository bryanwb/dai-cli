{
  "name": "dai-shell",
  "description": "An interactive shell and command-line interface for dai.js",
  "version": "0.0.7",
  "author": "Bryan W. Berry <bryan.berry@gmail.com>",
  "bin": {
    "dai": "./bin/dai"
  },
  "bugs": "https://github.com/git@github.com:bryanwb/dai-shell.git/issues",
  "dependencies": {
    "@makerdao/dai": "^0.9.10",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "configstore": "^4.0.0",
    "easy-table": "^1.1.1",
    "inquirer": "^6.2.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/node": "^10.12.18",
    "chai": "^4",
    "eslint": "^5.5",
    "eslint-config-oclif": "^3.1",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "bin/dai",
    "src"
  ],
  "homepage": "https://github.com/git@github.com:bryanwb/dai-shell.git",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "dai",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "git@github.com:bryanwb/dai-shell.git",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
